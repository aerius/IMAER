  </complexType>
  <element name="StandardMooringInlandShipping" substitutionGroup="IMAER:MooringInlandShipping" type="IMAER:StandardMooringInlandShippingType">
    <annotation>
      <documentation>General description of ships at an inland shipping mooring.
This class describes a ship at a mooring, using a predefined ship type.</documentation>
    </annotation>
  </element>
  <complexType name="StandardMooringInlandShippingType">
    <complexContent>
      <extension base="IMAER:MooringInlandShippingType">
        <sequence/>
        <attribute name="shipType" type="string" use="required">
          <annotation>
            <documentation>Ship type.</documentation>
          </annotation>
        </attribute>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="StandardMooringInlandShippingPropertyType">
    <sequence>
      <element ref="IMAER:StandardMooringInlandShipping"/>
    </sequence>
  </complexType>
  <element name="StandardMooringMaritimeShipping" substitutionGroup="IMAER:MooringMaritimeShipping" type="IMAER:StandardMooringMaritimeShippingType">
    <annotation>
      <documentation>General description of ships at a maritime shipping mooring.
This class describes a ship at a mooring, using a predefined ship type.</documentation>
    </annotation>
  </element>
  <complexType name="StandardMooringMaritimeShippingType">
    <complexContent>
      <extension base="IMAER:MooringMaritimeShippingType">
        <sequence/>
        <attribute name="shipType" type="string" use="required">
          <annotation>
            <documentation>Ship type.</documentation>
          </annotation>
        </attribute>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="StandardMooringMaritimeShippingPropertyType">
    <sequence>
      <element ref="IMAER:StandardMooringMaritimeShipping"/>
    </sequence>
  </complexType>
  <element name="StandardOffRoadMobileSource" substitutionGroup="IMAER:OffRoadMobileSource" type="IMAER:StandardOffRoadMobileSourceType">
    <annotation>
      <documentation>Characteristics of an emission type for mobile equipment.
Mobile tools are described on the basis of a number of characteristics. These are recorded in this data type.</documentation>
    </annotation>
  </element>
  <complexType name="StandardOffRoadMobileSourceType">
    <complexContent>
      <extension base="IMAER:OffRoadMobileSourceType">
        <sequence>
          <element minOccurs="0" name="literFuelPerYear" type="integer">
            <annotation>
              <documentation>Liters of fuel (summed) that this type of vehicle consumes per year.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="operatingHoursPerYear" type="integer">
            <annotation>
              <documentation>Time in hours per year that the vehicle is running.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="literAdBluePerYear" type="integer">
            <annotation>
              <documentation>Liters of AdBlue (summed) that this type of vehicle uses per year.</documentation>
            </annotation>
          </element>
        </sequence>
        <attribute name="offRoadMobileSourceType" type="string" use="required">
          <annotation>
            <documentation>The class of this mobile machinery.</documentation>
          </annotation>
        </attribute>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="StandardOffRoadMobileSourcePropertyType">
    <sequence>
      <element ref="IMAER:StandardOffRoadMobileSource"/>
    </sequence>
  </complexType>
  <element name="StandardTimeVaryingProfile" substitutionGroup="IMAER:TimeVaryingProfile" type="IMAER:StandardTimeVaryingProfileType">
    <annotation>
      <documentation>Temporal variation according to one of the standard values known within AERIUS (based on OPS).</documentation>
    </annotation>
  </element>
  <complexType name="StandardTimeVaryingProfileType">
    <complexContent>
      <extension base="IMAER:TimeVaryingProfileType">
        <sequence>
          <element name="standardType" type="string">
            <annotation>
              <documentation>The standard type to be used. Due to expandability, it is not specified which values are possible, but there is a limited possibility.</documentation>
            </annotation>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="StandardTimeVaryingProfilePropertyType">
    <sequence>
      <element ref="IMAER:StandardTimeVaryingProfile"/>
    </sequence>
  </complexType>
  <element name="StandardVehicle" substitutionGroup="IMAER:Vehicle" type="IMAER:StandardVehicleType">
    <annotation>
      <documentation>Characteristics of a standard vehicle type class.
This class describes a standard vehicle type group for moving traffic, using a predefined vehicle group code.</documentation>
    </annotation>
  </element>
  <complexType name="StandardVehicleType">
    <complexContent>
      <extension base="IMAER:VehicleType">
        <sequence>
          <element name="stagnationFactor" type="double">
            <annotation>
              <documentation>Fraction of traffic that is stagnant.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="maximumSpeed" type="integer">
            <annotation>
              <documentation>Maximum speed in kilometers per hour.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="strictEnforcement" type="boolean">
            <annotation>
              <documentation>Presence of strict speed enforcement.</documentation>
            </annotation>
          </element>
        </sequence>
        <attribute name="vehicleType" type="string" use="required">
          <annotation>
            <documentation>The class of this standard vehicle group.</documentation>
          </annotation>
        </attribute>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="StandardVehiclePropertyType">
    <sequence>
      <element ref="IMAER:StandardVehicle"/>
    </sequence>
  </complexType>
  <element name="SubPoint" substitutionGroup="IMAER:CalculationPointType" type="IMAER:SubPointType">
    <annotation>
      <documentation>Subpoint for a receptor point.
This class describes a sub-calculation point of a receptor point on the AERIUS grid.</documentation>
    </annotation>
  </element>
  <complexType name="SubPointType">
    <complexContent>
      <extension base="IMAER:CalculationPointTypeType">
        <sequence>
          <element name="level" type="integer">
            <annotation>
              <documentation>The level at which this sub-calculation point is determined. The closer to a source, the higher level and the more sub-calculation points are used for the receptor point.</documentation>
            </annotation>
          </element>
        </sequence>
        <attribute name="subPointId" type="integer" use="required">
          <annotation>
            <documentation>Identification number of the subpoint.</documentation>
          </annotation>
        </attribute>
        <attribute name="receptorPointId" type="integer" use="required">
          <annotation>
            <documentation>Identification number of the receptor point on which this sub-calculation point is based.</documentation>
          </annotation>
        </attribute>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="SubPointPropertyType">
    <sequence minOccurs="0">
      <element ref="IMAER:SubPoint"/>
    </sequence>
    <attributeGroup ref="gml:AssociationAttributeGroup"/>
    <attributeGroup ref="gml:OwnershipAttributeGroup"/>
  </complexType>
  <simpleType name="SubstanceType">
    <annotation>
      <documentation>List of different substance names.
The AERIUS calculation core can calculate emissions for a number of substances, which are listed here.</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="NH3">
        <annotation>
          <documentation>NH3 (ammonia).</documentation>
        </annotation>
      </enumeration>
      <enumeration value="NOX">
        <annotation>
          <documentation>NOx (nitrogen oxides).</documentation>
        </annotation>
      </enumeration>
      <enumeration value="NO2">
        <annotation>
          <documentation>NO2 (nitrogen dioxide).</documentation>
        </annotation>
      </enumeration>
      <enumeration value="PM10">
        <annotation>
          <documentation>PM10 (particulate matter with a diameter less than 10 microns).</documentation>
        </annotation>
      </enumeration>
      <enumeration value="PM25">
        <annotation>
          <documentation>PM2.5 (particulate matter with a diameter less than 2.5 microns).</documentation>
        </annotation>
      </enumeration>
      <enumeration value="EC">
        <annotation>
          <documentation>EC (elemental carbon).</documentation>
        </annotation>
      </enumeration>
      <enumeration value="NOXNH3">
        <annotation>
          <documentation>Combination of NOx and NH3, mostly used for Nitrogen deposition as a result of these substances.</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <simpleType name="TimeUnitType">
    <annotation>
      <documentation>List of different time units.
The AERIUS calculation core can calculate emissions based on time units, which are listed here.</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="HOUR"/>
      <enumeration value="DAY"/>
      <enumeration value="MONTH"/>
      <enumeration value="YEAR"/>
    </restriction>
  </simpleType>
  <element abstract="true" name="TimeVaryingProfile" substitutionGroup="gml:AbstractObject" type="IMAER:TimeVaryingProfileType">
    <annotation>
      <documentation>Temporal variation.</documentation>
    </annotation>
  </element>
  <complexType abstract="true" name="TimeVaryingProfileType">
    <sequence/>
  </complexType>
  <complexType name="TimeVaryingProfilePropertyType">
    <sequence>
      <element ref="IMAER:TimeVaryingProfile"/>
    </sequence>
  </complexType>
  <simpleType name="TrafficDirectionType">
    <annotation>
      <documentation>List of different directions for traffic on roads.
For a road, it can be indicated in which direction the traffic travels on the line segment in relation to the drawn direction. The options are included in this list.</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="BOTH">
        <annotation>
          <documentation>Traffic can move in both directions.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="A_TO_B">
        <annotation>
          <documentation>Traffic travels from A (beginning of the line) to B (end of the line).</documentation>
        </annotation>
      </enumeration>
      <enumeration value="B_TO_A">
        <annotation>
          <documentation>Traffic travels from B (end of the line) to A (beginning of the line).</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <simpleType name="TreeProfileType">
    <annotation>
      <documentation>List of different types of tree profile between the road and the calculation point.
When calculating emissions for road traffic in suburban areas, trees along the road can be taken into account (SRM1).</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="NONE_OR_FEW">
        <annotation>
          <documentation>Trees sparse or not present.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="SEPARATED">
        <annotation>
          <documentation>One or more rows of trees with separation of &lt; 15 meters with gaps between the crowns.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="PACKED">
        <annotation>
          <documentation>The crowns touch each other and span at least a third of the street width.</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <element abstract="true" name="Vehicle" substitutionGroup="IMAER:EmissionValueType" type="IMAER:VehicleType">
    <annotation>
      <documentation>Characteristics of a vehicle type.
There are different vehicle types available in the model. This abstract base class is a generalization of the different types in the model.</documentation>
    </annotation>
  </element>
  <complexType abstract="true" name="VehicleType">
    <complexContent>
      <extension base="IMAER:EmissionValueTypeType">
        <sequence>
          <element name="vehiclesPerTimeUnit" type="double">
            <annotation>
              <documentation>Number of vehicles per time unit.</documentation>
            </annotation>
          </element>
          <element name="timeUnit" type="IMAER:TimeUnitType">
            <annotation>
              <documentation>Time unit for the number of vehicles.</documentation>
            </annotation>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  <complexType name="VehiclePropertyType">
    <sequence>
      <element ref="IMAER:Vehicle"/>
    </sequence>
  </complexType>
  <element name="VersionMetadata" substitutionGroup="gml:AbstractObject" type="IMAER:VersionMetadataType">
    <annotation>
      <documentation>Metadata about version used.
This data type contains the versions used to arrive at the data.</documentation>
    </annotation>
  </element>
  <complexType name="VersionMetadataType">
    <sequence>
      <element name="aeriusVersion" type="string">
        <annotation>
          <documentation>Version number of the AERIUS model core.</documentation>
        </annotation>
      </element>
      <element name="databaseVersion" type="string">
        <annotation>
          <documentation>Version number of the AERIUS database.</documentation>
        </annotation>
      </element>
    </sequence>
  </complexType>
  <complexType name="VersionMetadataPropertyType">
    <sequence>
      <element ref="IMAER:VersionMetadata"/>
    </sequence>
  </complexType>
  <simpleType name="WaterwayDirectionTypeType">
    <annotation>
      <documentation>Options for the flow direction of routes for inland vessels.
The flow for an inland shipping route can count towards the emission factors, depending on the waterway type. If not relevant, the flow direction is determined as the drawing direction.</documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="IRRELEVANT">
        <annotation>
          <documentation>Flow is not important.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="UPSTREAM">
        <annotation>
          <documentation>Against the flow.</documentation>
        </annotation>
      </enumeration>
      <enumeration value="DOWNSTREAM">
        <annotation>
          <documentation>With the flow.</documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
</schema>
